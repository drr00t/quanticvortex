				<ResourceCompiler>
					<Add directory="..\..\include" />
					<Add directory="..\..\..\..\engine\include" />
					<Add directory="..\..\..\..\dependencies\include\UnitTest++" />
				</ResourceCompiler>
				<Linker>
					<Add option="/INCREMENTAL:NO" />
					<Add library="UnitTest++.vsnet2005" />
					<Add directory="..\..\..\..\dependencies\libs\release" />
				</Linker>
			</Target>
		</Build>
		<Unit filename="..\..\source\main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
