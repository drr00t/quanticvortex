			<Target title="Debug Win32">
				<Option output="engine tests" prefix_auto="1" extension_auto="1" />
				<Option object_output="..\..\obj\debug" />
				<Option type="1" />
				<Option compiler="msvc8" />
				<Compiler>
					<Add option="/DWIN32" />
					<Add option="/D_DEBUG" />
					<Add option="/D_CONSOLE" />
					<Add option="/W3" />
					<Add option="/ZI" />
					<Add option="/Od" />
