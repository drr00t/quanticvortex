					<Add directory="..\..\include" />
					<Add directory="..\..\..\..\engine\include" />
					<Add directory="..\..\..\..\dependencies\include\UnitTest++" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="..\..\include" />
					<Add directory="..\..\..\..\engine\include" />
					<Add directory="..\..\..\..\dependencies\include\UnitTest++" />
